<Window x:Class="SafariExplorerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SafariExplorerWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
	<StackPanel>
		<Frame x:Name="_mainFrame"/>
		<Grid Height="720" Width="1280">
			<Grid.Background>
				<ImageBrush ImageSource="/BackGround_Savanna.jpg"/>
			</Grid.Background>
			<TextBlock x:Name="TxtBlockTitle" HorizontalAlignment="Center" Margin="0,101,0,0" Text="Safari Explorer" TextWrapping="Wrap" VerticalAlignment="Top" FontFamily="Castellar" FontSize="90" FontWeight="Bold" Foreground="#E5000000"/>
			<Button x:Name="Btn_GoToMenu" Content="Start" HorizontalAlignment="Center" Margin="0,420,0,0" VerticalAlignment="Top" Height="108" Width="216" FontFamily="Freestyle Script" FontSize="72" BorderBrush="#66060000" Click="BtnClick_GoToMenu">
				<Button.Background>
					<ImageBrush ImageSource="/Paper_Texture.png"/>
				</Button.Background>
			</Button>


		</Grid>
	</StackPanel>
</Window>
