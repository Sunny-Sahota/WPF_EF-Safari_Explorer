<Page x:Class="SafariExplorerWPF.SafariQuiz"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SafariExplorerWPF"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280"
      Title="SafariQuiz">

	<StackPanel>
		<Frame x:Name="_SafariQuiz"/>
		<Grid Height="720" Width="1280">
			<Grid.Background>
				<ImageBrush ImageSource="/BackGround_Savanna.jpg"/>
			</Grid.Background>
			<TextBlock HorizontalAlignment="Left" Margin="554,60,0,0" Text="Safari Quiz" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="40"/>

			<Rectangle HorizontalAlignment="Left" Height="240" Margin="244,142,0,0" Stroke="Black" VerticalAlignment="Top" Width="800" Fill="#7F000000"/>
			<TextBox x:Name="TxtBoxQuestions" HorizontalAlignment="Left" Margin="264,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="757" Height="197" IsEnabled="False"/>
		</Grid>


	</StackPanel>
</Page>
